#4.1#
#Model for Normal (0,1)#
Model1_Normal <- function (N, ii) { 
#values for the sample#
ner = 1:8
Trial1 =matrix (,ii,8, byrow=TRUE)  
dimnames(Trial1) = list(1:ii ,c("Mean","T.1N","T.2N","W.1N","W.2N","L.1N","L.2N","Median") )

TrialVariance1 =matrix (,ii,8, byrow=TRUE)  
dimnames(TrialVariance1) = list(1:ii ,c("Mean","T.1N","T.2N","W.1N","W.2N","L.1N","L.2N","Median") )

#Generate the Random Values#
   for (index in 1:ii){
      GenData <- rnorm(N, 0 ,1)
      # Means#
         Trial1[index,ner[1]] <- mean(GenData)
      #MeanVariance#
         TrialVariance1[index,ner[1]] <- var(GenData)
      #Trimmed .1#
         Trial1[index,ner[2]] <- rTrimmed (GenData,.1)
      #Trimmed .1 Variance# 
         TrialVariance1[index,ner[2]]<- sum((GenData - Trial1[index,ner[2]])^2)/N
      #Trimmed .2#
         Trial1[index,ner[3]] <- rTrimmed(GenData,.2)
      #Trimmed .2 Variance#
         TrialVariance1[index,ner[3]] <- sum((GenData - Trial1[index,ner[3]])^2)/N
      #Winsored .1#
         Trial1[index,ner[4]] <- rWinsorized(GenData,.1)
      #Winsored .1 Variance#
         TrialVariance1[index,ner[4]] <- sum((GenData - Trial1[index,ner[4]])^2)/N
      #Winsored .2#
         Trial1[index,ner[5]]<- rWinsorized(GenData,.2)
      #Winsored .2 Variance# 
         TrialVariance1[index,ner[5]] <- sum((GenData - Trial1[index,ner[5]])^2)/N
      #Linearly .1#
         Trial1[index,ner[6]] <- rLinearly(GenData,.1)
      #Linearly .1 Variance# 
        TrialVariance1[index,ner[6]] <- sum((GenData - Trial1[index,ner[6]])^2)/N
      #Linearly .2#
         Trial1[index,ner[7]]<- rLinearly(GenData,.2)
      #Linearly .2 Variance#
         TrialVariance1[index,ner[7]] <- sum((GenData - Trial1[index,ner[7]])^2)/N
      #Median#
         Trial1[index,ner[8]]<- median(GenData)
      #Median#
         TrialVariance1[index,ner[8]] <- sum((GenData - Trial1[index,ner[8]])^2)/N     
    }  
Trial <- list (Trial1, TrialVariance1)
GenData = NULL 
print(paste("Finished Normal Distribution N =", N, " Iterations =", index))
return(Trial)
}


