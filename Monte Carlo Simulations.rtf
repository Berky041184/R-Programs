#----------------------------------Evaluating Models by Means,Variance,MSE -----------------------------#\par
# This function will give us the Expected Value of all the Means and their Variance and Standard #Deviation\par
\par
#The input is the Trial vector \endash  the vector that has the expected means from the above functions\par
#Find Bias, MSE and Variance# \par
Mean_Variance <- function(Trial) \{\par
Trial <- Trial[1]\par
Trial <- matrix(unlist(Trial),,8)\par
\par
#values for final MSE and Variance#\par
ner<-1:8\par
ModelMean = matrix(,3,8)\par
dimnames(ModelMean) = list(c("mean", "variance","SD"),c("Mean","T.1N","T.2N","W.1N","W.2N","L.1N","L.2N","Median") ) \par
\par
#Find Means and Variances#\par
ModelMean[1,1] = mean(Trial[,ner[1]])\par
ModelMean[2,1]= var(Trial[,ner[1]])\par
ModelMean[3,1]= sqrt(ModelMean[2,1])\par
\par
ModelMean[1,2]= mean(Trial[,ner[2]])\par
ModelMean[2,2]= var(Trial[,ner[2]])\par
ModelMean[3,2]= sqrt(ModelMean[2,2])\par
\par
ModelMean[1,3] = mean(Trial[,ner[3]])\par
ModelMean[2,3]= var(Trial[,ner[3]])\par
ModelMean[3,3]= sqrt(ModelMean[2,3])\par
\par
\par
ModelMean[1,4]= mean(Trial[,ner[4]])\par
ModelMean[2,4] = var(Trial[,ner[4]])\par
ModelMean[3,4]= sqrt(ModelMean[2,4])\par
\par
ModelMean[1,5] = mean(Trial[,ner[5]])\par
ModelMean[2,5] = var(Trial[,ner[5]])\par
ModelMean[3,5]= sqrt(ModelMean[2,5])\par
\par
ModelMean[1,6]= mean(Trial[,ner[6]])\par
ModelMean[2,6]= var(Trial[,ner[6]])\par
ModelMean[3,6]= sqrt(ModelMean[2,6])\par
\par
ModelMean[1,7] = mean(Trial[,ner[7]])\par
ModelMean[2,7] = var(Trial[,ner[7]])\par
ModelMean[3,7]= sqrt(ModelMean[2,7])\par
\par
ModelMean[1,8] = mean(Trial[,ner[8]])\par
ModelMean[2,8] = var(Trial[,ner[8]]) \par
ModelMean[3,8]= sqrt(ModelMean[2,8])\par
\par
return(ModelMean)\par
\}\par
\par
#This Function will give us the average of all the Standard Errors that came from the above functions\par
# It takes in the Standard Errors and Averages them out \par
\par
Average_SE <- function(Trial, samplesize) \{\par
# It pulls the second list (which has the Standard Errors and put it into a vector) \par
Trial <- Trial[2]\par
Trial <- matrix(unlist(Trial),,8)\par
#values for final MSE and Variance#\par
ner<-1:8\par
N1<-samplesize\par
AverageSE = matrix(,1,8)\par
dimnames(AverageSE) = list(c("average SE"),c("Mean","T.1N","T.2N","W.1N","W.2N","L.1N","L.2N","Median") ) \par
\par
#Find Means and Variances#\par
AverageSE [,1] = mean(sqrt(Trial[,1]/N1))\par
AverageSE [,2]= mean(sqrt(Trial[,2]/N1))\par
AverageSE [,3] = mean(sqrt(Trial[,3]/N1))\par
AverageSE [,4]= mean(sqrt(Trial[,4]/N1))\par
AverageSE [,5] = mean(sqrt(Trial[,5]/N1))\par
AverageSE [,6]= mean(sqrt(Trial[,6]/N1))\par
AverageSE [,7] = mean(sqrt(Trial[,7]/N1))\par
AverageSE [,8] = mean(sqrt(Trial[,8]/N1))\par
 \par
return (AverageSE)\par
\}\par
\par
#MSE Calcuations \endash  It should give us the MSE\par
\par
MSE_Calculation<- function(ModelMean)\{\par
ii <- length(ModelMean[1,])\par
MSE = matrix(,1,8)\par
dimnames(MSE) = list(c("MSE") ,c("Mean","T.1N","T.2N","W.1N","W.2N","L.1N","L.2N","Median") ) \par
\par
     for(index4 in 1:ii)\{\par
       Bias <- ModelMean[1,index4]\par
       Var <- ModelMean[2,index4]\par
       #It pulls in the E(means) and Variance from the Means_Variance Function and \par
       MSE[1,index4]<- Bias^2 + Var\par
     \}\par
 \par
return(MSE)\par
\}\par
