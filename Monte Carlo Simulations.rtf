MSE_Calculation<- function(ModelMean){
ii <- length(ModelMean[1,])
MSE = matrix(,1,8)
dimnames(MSE) = list(c("MSE") ,c("Mean","T.1N","T.2N","W.1N","W.2N","L.1N","L.2N","Median") ) 

     for(index4 in 1:ii){
       Bias <- ModelMean[1,index4]
       Var <- ModelMean[2,index4]
       #It pulls in the E(means) and Variance from the Means_Variance Function and 
       MSE[1,index4]<- Bias^2 + Var
     }
 
return(MSE)
}

