ModelMean[3,8]= sqrt(ModelMean[2,8])

return(ModelMean)
}

#This Function will give us the average of all the Standard Errors that came from the above functions
# It takes in the Standard Errors and Averages them out 

Average_SE <- function(Trial, samplesize) {
# It pulls the second list (which has the Standard Errors and put it into a vector) 
Trial <- Trial[2]
Trial <- matrix(unlist(Trial),,8)
#values for final MSE and Variance#
ner<-1:8
N1<-samplesize
