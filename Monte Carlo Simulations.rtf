#3.1# Fixed Outlier 
FixedOutlier<- function(k1, kper) {
X1<-rnorm(k1,0,1)
H1a<- X1[1:kper]*2
H1b<- X1[(kper+1): k1]
H1c<- c(H1a,H1b) 
return(H1c)
}

#3.2# Random Outlier
RandomOutlier<- function (k2, per){
X2<- rbinom(1,k2,per)
Z2<-rnorm(k2,0,1)
H2a<- Z2[1:X2]*2
H2b<- Z2[(X2+1):k2]
H2c<- c(H2a,H2b) 
Rlist = list(H2c, X2) 
return (Rlist) 
}

#3.3# Logistic
LogisticModel<- function (k3){
U3<- runif(k3,0,1)
Y3<- log((U3/(1-U3)))
return(Y3)
}

#3.4# Laplace Distribution
LaplaceModel<- function (k4){

U4<- runif(k4,0,1)
U5<- runif(k4,0,1)

W1<-  -log(U4)
W2<-  -log(U5)

Y4 <- W1-W2
return(Y4)
}

