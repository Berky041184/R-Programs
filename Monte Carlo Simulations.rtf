dimnames(AverageSE) = list(c("average SE"),c("Mean","T.1N","T.2N","W.1N","W.2N","L.1N","L.2N","Median") ) 

#Find Means and Variances#
AverageSE [,1] = mean(sqrt(Trial[,1]/N1))
AverageSE [,2]= mean(sqrt(Trial[,2]/N1))
AverageSE [,3] = mean(sqrt(Trial[,3]/N1))
AverageSE [,4]= mean(sqrt(Trial[,4]/N1))
AverageSE [,5] = mean(sqrt(Trial[,5]/N1))
