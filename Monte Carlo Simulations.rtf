#Model for Normal (0,1)#\par
Model1_Normal <- function (N, ii) \{ \par
#values for the sample#\par
ner = 1:8\par
Trial1 =matrix (,ii,8, byrow=TRUE)  \par
dimnames(Trial1) = list(1:ii ,c("Mean","T.1N","T.2N","W.1N","W.2N","L.1N","L.2N","Median") )\par
\par
TrialVariance1 =matrix (,ii,8, byrow=TRUE)  \par
dimnames(TrialVariance1) = list(1:ii ,c("Mean","T.1N","T.2N","W.1N","W.2N","L.1N","L.2N","Median") )\par
\par
#Generate the Random Values#\par
   for (index in 1:ii)\{\par
      GenData <- rnorm(N, 0 ,1)\par
      # Means#\par
         Trial1[index,ner[1]] <- mean(GenData)\par
      #MeanVariance#\par
         TrialVariance1[index,ner[1]] <- var(GenData)\par
      #Trimmed .1#\par
         Trial1[index,ner[2]] <- rTrimmed (GenData,.1)\par
      #Trimmed .1 Variance# \par
         TrialVariance1[index,ner[2]]<- sum((GenData - Trial1[index,ner[2]])^2)/N\par
      #Trimmed .2#\par
         Trial1[index,ner[3]] <- rTrimmed(GenData,.2)\par
      #Trimmed .2 Variance#\par
         TrialVariance1[index,ner[3]] <- sum((GenData - Trial1[index,ner[3]])^2)/N\par
      #Winsored .1#\par
         Trial1[index,ner[4]] <- rWinsorized(GenData,.1)\par
      #Winsored .1 Variance#\par
         TrialVariance1[index,ner[4]] <- sum((GenData - Trial1[index,ner[4]])^2)/N\par
      #Winsored .2#\par
         Trial1[index,ner[5]]<- rWinsorized(GenData,.2)\par
      #Winsored .2 Variance# \par
         TrialVariance1[index,ner[5]] <- sum((GenData - Trial1[index,ner[5]])^2)/N\par
      #Linearly .1#\par
         Trial1[index,ner[6]] <- rLinearly(GenData,.1)\par
      #Linearly .1 Variance# \par
        TrialVariance1[index,ner[6]] <- sum((GenData - Trial1[index,ner[6]])^2)/N\par
      #Linearly .2#\par
         Trial1[index,ner[7]]<- rLinearly(GenData,.2)\par
      #Linearly .2 Variance#\par
         TrialVariance1[index,ner[7]] <- sum((GenData - Trial1[index,ner[7]])^2)/N\par
      #Median#\par
         Trial1[index,ner[8]]<- median(GenData)\par
      #Median#\par
         TrialVariance1[index,ner[8]] <- sum((GenData - Trial1[index,ner[8]])^2)/N     \par
    \}  \par
Trial <- list (Trial1, TrialVariance1)\par
GenData = NULL \par
print(paste("Finished Normal Distribution N =", N, " Iterations =", index))\par
return(Trial)\par
\}\line\par
