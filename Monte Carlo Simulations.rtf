#2.1 r-Trimmed\par

\pard\nowidctlpar rTrimmed <- function(x,r)  \{ \par
x<-sort(x)\par
n1 <-length(x) \par
r<- n1*r\par
rTrim<-(sum(x[(1+r):(n1-r)]) /(n1-2*r))\par
return (rTrim)  \}\par

\pard\nowidctlpar\sa200\sl276\slmult1\tx930  \tab\par

\pard\nowidctlpar\sa200\sl276\slmult1 #2.2 rWinsorized\par

\pard\nowidctlpar\sl276\slmult1 rWinsorized<- function(x,r)\{\par
x<-sort(x)\par
n2 <-length(x)\par

\pard\nowidctlpar r<-n2*r\par
rWins<- (sum(x[(1+r)]*(r+1), x[(2+r):(n2-r-1)],x[(n2-r)]*(r+1)))/n2\par
return (rWins)\}\par
\par
#2.3 rLinearly\par

\pard\nowidctlpar\sl276\slmult1\par
rLinearly <- function(x,r) \{\par
n3 <- length(x) \par

\pard\nowidctlpar r<-n3*r\par
nn3<-sort(x)\par
weight2 = NULL\par
\par

\pard\nowidctlpar\sl276\slmult1 for ( ii in 1: ((n3/2)-r))  \par
\{ weight2[ii] <- ((2*ii-1)* (nn3[r+ii] + nn3[n3-r-ii+1]) ) \par
\}\par
rLins <- sum(weight2) /(2*((n3/2)-r)^2) \par
\par
return(rLins)\par
\} \par

