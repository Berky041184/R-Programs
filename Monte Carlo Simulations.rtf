#------------------Generate Random Number Models-----------------------#\par
\par
#3.1# Fixed Outlier \par
FixedOutlier<- function(k1, kper) \{\par
X1<-rnorm(k1,0,1)\par
H1a<- X1[1:kper]*2\par
H1b<- X1[(kper+1): k1]\par
H1c<- c(H1a,H1b) \par
return(H1c)\par
\}\par
\highlight1\par
\highlight0 #3.2# Random Outlier\par
RandomOutlier<- function (k2, per)\{\par
X2<- rbinom(1,k2,per)\par
Z2<-rnorm(k2,0,1)\par
H2a<- Z2[1:X2]*2\par
H2b<- Z2[(X2+1):k2]\par
H2c<- c(H2a,H2b) \par
Rlist = list(H2c, X2) \par
return (Rlist) \par
\}\par
\par
#3.3# Logistic\par
LogisticModel<- function (k3)\{\par
U3<- runif(k3,0,1)\par
Y3<- log((U3/(1-U3)))\par
return(Y3)\par
\}\par
\par
#3.4# Laplace Distribution\par
LaplaceModel<- function (k4)\{\par
\par
U4<- runif(k4,0,1)\par
U5<- runif(k4,0,1)\par
\par
W1<-  -log(U4)\par
W2<-  -log(U5)\par
\par
Y4 <- W1-W2\par
return(Y4)\par
\}\par
