#2.1 r-Trimmed
rTrimmed <- function(x,r)  { 
x<-sort(x)
n1 <-length(x) 
r<- n1*r
rTrim<-(sum(x[(1+r):(n1-r)]) /(n1-2*r))
return (rTrim)  }
 	
#2.2 rWinsorized
rWinsorized<- function(x,r){
x<-sort(x)
n2 <-length(x)
r<-n2*r
rWins<- (sum(x[(1+r)]*(r+1), x[(2+r):(n2-r-1)],x[(n2-r)]*(r+1)))/n2
return (rWins)}

#2.3 rLinearly

rLinearly <- function(x,r) {
n3 <- length(x) 
r<-n3*r
nn3<-sort(x)
weight2 = NULL

for ( ii in 1: ((n3/2)-r))  
{ weight2[ii] <- ((2*ii-1)* (nn3[r+ii] + nn3[n3-r-ii+1]) ) 
}
rLins <- sum(weight2) /(2*((n3/2)-r)^2) 

return(rLins)
} 

